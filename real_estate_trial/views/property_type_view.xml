<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record model="ir.ui.view" id="type_tree_view">
            <field name="name">property.type.tree</field>
            <field name="model">estate.property.type</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="sequence" widget="handle"/>
                    <!--                    <field name="offer_count"/>-->
                    <field name="name"/>
                </tree>
            </field>
        </record>
        <record model="ir.ui.view" id="type_form_view">
            <field name="name">property.type.form</field>
            <field name="model">estate.property.type</field>
            <field name="arch" type="xml">
                <form>

                    <sheet>
                        <group>
                            <notebook>
                                <page name="Description">
                                    <field name="property_ids">
                                        <tree>
                                            <field name="title"/>
                                            <field name="expected_price"/>
                                            <field name="status"/>
                                        </tree>
                                    </field>
                                </page>
                            </notebook>
                        </group>
                        <group>
                            <!--                            <field name="property_offer_ids" invisible="1"/>-->
                            <!--                            <field name="property_ids" invisible="1"/>-->
                            <field name="name"/>
                            <field name="sequence"/>
                        </group>


                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>