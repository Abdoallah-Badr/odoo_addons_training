<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record model="ir.actions.act_window" id="property_action">
        <field name="name">property</field>
        <field name="res_model">estate.property</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">Enter first property at your app</p>
        </field>
    </record>

    <menuitem id="main_estate_menu" name="Real Estate"/>
    <menuitem id="estate_menu" name="Real estate menu" parent="main_estate_menu"/>
    <menuitem id="estate_detailed_menu" name="Estate sub menu " parent="estate_menu"/>
    <menuitem id="property_menu" name="Property" parent="estate_detailed_menu" action="property_action"/>
</odoo>

