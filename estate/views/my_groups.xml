<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="egypt_group" model="ir.module.category">
            <field name="name">Egypt Categoury</field>
            <field name="description">Helps you manage your manufacturing processes and generate reports on those
                processes.
            </field>
            <field name="sequence">5</field>
        </record>


        <record id="benha_group" model="res.groups">
            <field name="name">Benha Groups</field>
            <field name="category_id" ref="estate.egypt_group"/>
        </record>

        <record id="benha_manager_group" model="res.groups">
            <field name="name">Benha Managers</field>
            <field name="category_id" ref="estate.egypt_group"/>
            <field name="implied_ids" eval="[(4, ref('benha_group'))]"/>
        </record>
    </data>

    <record id="test_record_rule" model="ir.rule">
        <field name="name">Owner Rule</field>
        <field name="model_id" ref="model_estate_property"/>
        <field name="domain_force">['buyer_id.user_id','=','user.id']</field>
        <field name="groups" eval="[(4, ref('benha_group'))]"/>

    </record>
</odoo>